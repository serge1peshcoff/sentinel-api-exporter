# HELP sentinel_api_block_height Block height
# TYPE sentinel_api_block_height gauge
sentinel_api_block_height <%= generalStats.block_height %>

# HELP sentinel_api_block_time Block time
# TYPE sentinel_api_block_time gauge
sentinel_api_block_time <%= generalStats.block_time %>

# HELP sentinel_api_total_txs_num Total txs num
# TYPE sentinel_api_total_txs_num gauge
sentinel_api_total_txs_num <%= generalStats.total_txs_num %>

# HELP sentinel_api_total_validator_num Total validator num
# TYPE sentinel_api_total_validator_num gauge
sentinel_api_total_validator_num <%= generalStats.total_validator_num %>

# HELP sentinel_api_unjailed_validator_num Unjailed validator num
